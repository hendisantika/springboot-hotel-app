                                    </li>
                                </ul>
                                <div class="tabled_widget_footer text-center">
                                    <a href="#" class="readmore">Read More</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</section>

#end

#@scriptLayout()
